@model List<string>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Here you can find temperature data for your chosen location from a number of providers</p>

    @foreach(var supplierText in @Model)
    {
        <p>@supplierText</p>
    }
</div>
